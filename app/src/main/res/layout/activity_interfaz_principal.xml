<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".interfaz_principal">

    <FrameLayout
        android:layout_width="350dp"
        android:layout_height="90dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.114"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.329">
        <Button
            android:id="@+id/button2"
            android:layout_width="350dp"
            android:layout_height="90dp"
            android:background="@drawable/boton_redondo"
            android:fontFamily="@font/caviardreamsbold"
            android:text="Encontrar especialistas     mas cercanos"
            android:textColor="@color/B"
            android:onClick="maps"
            android:textSize="16dp"
            app:backgroundTint="#99C2DA"/>
    </FrameLayout>

    <FrameLayout
        android:layout_width="350dp"
        android:layout_height="90dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.114"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.829">

        <Button
            android:id="@+id/button4"
            android:layout_width="350dp"
            android:layout_height="90dp"
            android:background="@drawable/boton_redondo"
            android:fontFamily="@font/caviardreamsbold"
            android:text="tengo una emergencia"
            android:textColor="@color/B"
            android:onClick="emer"
            android:textSize="17dp"
            android:visibility="visible"
            app:backgroundTint="#99C2DA"/>
    </FrameLayout>


    <FrameLayout
        android:layout_width="110dp"
        android:layout_height="42dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.263"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.976">
        <Button
            android:id="@+id/btnsalir"
            android:layout_width="110dp"
            android:layout_height="42dp"
            android:background="@drawable/boton_redondo"
            android:fontFamily="@font/caviardreamsbold"
            android:gravity="right"
            android:text="Salir"
            android:textColor="@color/B"
            android:textSize="19dp"
            app:backgroundTint="#99C2DA" />



    </FrameLayout>

    <FrameLayout
        android:layout_width="350dp"
        android:layout_height="90dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.114"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.092">

        <Button
            android:id="@+id/button"
            android:layout_width="350dp"
            android:layout_height="90dp"
            android:background="@drawable/boton_redondo"
            android:fontFamily="@font/caviardreamsbold"
            android:gravity="center"
            android:onClick="info"
            android:text="Informacion de      especialistas"
            android:textColor="@color/B"
            android:textSize="17dp"
            app:backgroundTint="#99C2DA" />

    </FrameLayout>

    <FrameLayout
        android:layout_width="350dp"
        android:layout_height="90dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.114"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.581">
        <Button
            android:id="@+id/button3"
            android:layout_width="350dp"
            android:layout_height="90dp"
            android:background="@drawable/boton_redondo"
            android:fontFamily="@font/caviardreamsbold"
            android:text="calculadora de imc"
            android:onClick="cal"
            android:textColor="@color/B"
            android:textSize="17dp"
            app:backgroundTint="#99C2DA"/>

    </FrameLayout>

    <FrameLayout
        android:layout_width="64dp"
        android:layout_height="64dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.953"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.109">
        <ImageButton
            android:id="@+id/imageButton4"
            android:layout_width="64dp"
            android:layout_height="64dp"
            android:onClick="info"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.953"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.165"
            app:srcCompat="@drawable/iconodoctor" />
    </FrameLayout>

    <FrameLayout
        android:layout_width="64dp"
        android:layout_height="64dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.953"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.34">

        <ImageButton
            android:id="@+id/imageButton5"
            android:layout_width="64dp"
            android:layout_height="64dp"
            android:onClick="maps"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.953"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.36"
            app:srcCompat="@drawable/iconoubi" />
    </FrameLayout>

    <FrameLayout
        android:layout_width="64dp"
        android:layout_height="64dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.953"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.579">
        <ImageButton
            android:id="@+id/imageButton3"
            android:layout_width="64dp"
            android:layout_height="64dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.953"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:onClick="cal"
            app:layout_constraintVertical_bias="0.541"
            app:srcCompat="@drawable/iconocalculadora" />
    </FrameLayout>

    <FrameLayout
        android:layout_width="64dp"
        android:layout_height="64dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.953"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.813">

        <ImageButton
            android:id="@+id/imageButton2"
            android:layout_width="64dp"
            android:layout_height="64dp"
            android:onClick="emer"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.72"
            app:srcCompat="@drawable/iconoemergencia" />
    </FrameLayout>

    <FrameLayout
        android:layout_width="64dp"
        android:layout_height="64dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.109"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.991">
        <ImageButton
            android:id="@+id/imgsalir"
            android:layout_width="64dp"
            android:layout_height="64dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.72"
            app:srcCompat="@drawable/iconosalir" />


    </FrameLayout>


</androidx.constraintlayout.widget.ConstraintLayout>