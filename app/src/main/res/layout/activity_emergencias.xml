                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/caviardreams"
                android:gravity="center"
                android:text="Emergencia EsSalud"
                android:textColor="@color/rosao"
                android:textSize="30dp" />

            <Button
                android:id="@+id/btnllamar1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Llamar" />

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/lineaga"
                android:layout_marginVertical="30dp"/>

            <TextView
                android:id="@+id/textView19"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/caviardreams"
                android:gravity="center"
                android:text="Emergencia Covid -19 EsSalud"
                android:textColor="@color/rosao"
                android:textSize="30dp" />

            <Button
                android:id="@+id/btnllamar2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Llamar" />

            <ImageView
                android:id="@+id/imageView12"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/lineaga"
                android:layout_marginVertical="30dp"/>

            <TextView
                android:id="@+id/textView20"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/caviardreams"
                android:gravity="center"
                android:text="Bomberos Cusco"
                android:textColor="@color/rosao"
                android:textSize="30dp" />

            <Button
                android:id="@+id/btnllamar3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Llamar" />

            <ImageView
                android:id="@+id/imageView13"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/lineaga"
                android:layout_marginVertical="30dp"/>

            <TextView
                android:id="@+id/textView21"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/caviardreams"
                android:gravity="center"
                android:text="Policia Cusco"
                android:textColor="@color/rosao"
                android:textSize="30dp"/>

            <Button
                android:id="@+id/btnllamar4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Llamar" />

            <ImageView
                android:id="@+id/imageView14"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/lineaga"
                android:layout_marginVertical="30dp"/>

            <TextView
                android:id="@+id/textView22"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/caviardreams"
                android:gravity="center"
                android:text="Emergencia Clinica Pardo Cusco"
                android:textColor="@color/rosao"
                android:textSize="30dp" />

            <Button
                android:id="@+id/btnllamar5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Llamar" />


        </LinearLayout>


    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>