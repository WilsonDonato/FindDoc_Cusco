            android:layout_height="wrap_content"
            android:orientation="vertical">


            <Button
                android:id="@+id/button6"
                android:layout_width="350dp"
                android:layout_height="50dp"
                android:background="@drawable/boton_redondo"
                android:fontFamily="@font/caviardreamsbold"
                android:gravity="center"
                android:text="Medicos generales"
                android:onClick="primero"
                android:textColor="@color/B"
                android:textSize="17dp"
                app:backgroundTint="#99C2DA"
                android:layout_marginVertical="20dp" />

            <Button
                android:id="@+id/button7"
                android:layout_width="350dp"
                android:layout_height="50dp"
                android:background="@drawable/boton_redondo"
                android:fontFamily="@font/caviardreamsbold"
                android:gravity="center"
                android:text="Enfermeros"
                android:onClick="segundo"
                android:textColor="@color/B"
                android:textSize="17dp"
                app:backgroundTint="#99C2DA"
                android:layout_marginVertical="20dp"/>

            <Button
                android:id="@+id/button8"
                android:layout_width="350dp"
                android:layout_height="50dp"
                android:background="@drawable/boton_redondo"
                android:fontFamily="@font/caviardreamsbold"
                android:gravity="center"
                android:onClick="tercero"

                android:text="Cardiologos"
                android:textColor="@color/B"
                android:textSize="17dp"
                app:backgroundTint="#99C2DA"
                android:layout_marginVertical="20dp"/>

            <Button
                android:id="@+id/button9"
                android:layout_width="350dp"
                android:layout_height="50dp"
                android:background="@drawable/boton_redondo"
                android:fontFamily="@font/caviardreamsbold"
                android:gravity="center"
                android:onClick="cuarto"

                android:text="Dermatologos"
                android:textColor="@color/B"
                android:textSize="17dp"
                app:backgroundTint="#99C2DA"
                android:layout_marginVertical="20dp" />

            <Button
                android:id="@+id/button10"
                android:layout_width="350dp"
                android:layout_height="50dp"
                android:layout_marginVertical="20dp"
                android:background="@drawable/boton_redondo"
                android:fontFamily="@font/caviardreamsbold"
                android:gravity="center"
                android:onClick="quinto"

                android:text="Endocrinologos"
                android:textColor="@color/B"
                android:textSize="17dp"
                app:backgroundTint="#99C2DA" />

            <Button
                android:id="@+id/button11"
                android:layout_width="350dp"
                android:layout_height="50dp"
                android:background="@drawable/boton_redondo"
                android:fontFamily="@font/caviardreamsbold"
                android:gravity="center"
                android:text="Cirugia General"
                android:textColor="@color/B"
                android:textSize="17dp"
                app:backgroundTint="#99C2DA"
                android:layout_marginVertical="20dp" />
            <Button
                android:id="@+id/button12"
                android:layout_width="350dp"
                android:layout_height="50dp"
                android:background="@drawable/boton_redondo"
                android:fontFamily="@font/caviardreamsbold"
                android:gravity="center"
                android:text="Neurologia"
                android:textColor="@color/B"
                android:textSize="17dp"
                app:backgroundTint="#99C2DA"
