<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".lista_dermatologos">

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="0.1"
        android:fontFamily="@font/caviardreamsbold"
        android:gravity="center"
        android:text="Dermatologos "
        android:textColor="@color/rosao"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.094" />

    <ScrollView
        android:layout_width="405dp"
        android:layout_height="556dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.691">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="10dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imge1"
                    android:layout_width="0dp"
                    android:layout_height="90dp"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/docm" />

                <TextView
                    android:id="@+id/txte1"
                    android:layout_width="160dp"
                    android:layout_height="90dp"
                    android:layout_weight="1"
                    android:background="#00FFFFFF"
                    android:gravity="center"
                    android:onClick="yaracely"

                    android:fontFamily="@font/caviardreams"
                    android:text="Lic. Yulieh Ccala Ccopa"
                    android:textColor="@color/rosao"
                    android:textSize="20dp" />
            </LinearLayout>

            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/lineaga"
                android:layout_marginVertical="20dp"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imge2"
                    android:layout_width="0dp"
                    android:layout_height="90dp"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/docm" />

                <TextView
                    android:id="@+id/txte2"
                    android:layout_width="160dp"
                    android:layout_height="90dp"
                    android:layout_weight="1"
                    android:background="#00FFFFFF"
                    android:gravity="center"
                    android:onClick="segundo"

                    android:text="Lic. Blanca Ccama Huaranca"
                    android:fontFamily="@font/caviardreams"
                    android:textColor="@color/rosao"
                    android:textSize="20dp" />
            </LinearLayout>

            <ImageView

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/lineaga"
                android:layout_marginVertical="20dp"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imge3"
                    android:layout_width="0dp"
                    android:layout_height="90dp"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/docm" />

                <TextView
                    android:id="@+id/txte3"
                    android:layout_width="160dp"
                    android:layout_height="90dp"
                    android:layout_weight="1"
                    android:background="#00FFFFFF"
                    android:gravity="center"
                    android:onClick="tercero"

                    android:text="Lic. Alexandra Mercedes Concha"
                    android:fontFamily="@font/caviardreams"
                    android:textColor="@color/rosao"
                    android:textSize="20dp" />
            </LinearLayout>

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/lineaga"
                android:layout_marginVertical="20dp"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imge4"
                    android:layout_width="0dp"
                    android:layout_height="90dp"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/docm" />
                <TextView
                    android:id="@+id/txte4"
                    android:layout_width="160dp"
                    android:layout_height="90dp"
                    android:layout_weight="1"
                    android:background="#00FFFFFF"
                    android:gravity="center"
                    android:onClick="cuarto"

                    android:text="Lic. Flor Condo Peralta"
                    android:fontFamily="@font/caviardreams"
                    android:textColor="@color/rosao"
                    android:textSize="20dp" />
            </LinearLayout>

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/lineaga"
                android:layout_marginVertical="20dp"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imge5"
                    android:layout_width="0dp"
                    android:layout_height="90dp"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/doch" />
                <TextView
                    android:id="@+id/txte5"
                    android:layout_width="160dp"
                    android:layout_height="90dp"
                    android:layout_weight="1"
                    android:background="#00FFFFFF"
                    android:gravity="center"
                    android:onClick="quinto"

                    android:text="Lic. Max Cuadros Gutierrez"
                    android:fontFamily="@font/caviardreams"
                    android:textColor="@color/rosao"
                    android:textSize="20dp" />
            </LinearLayout>


        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>